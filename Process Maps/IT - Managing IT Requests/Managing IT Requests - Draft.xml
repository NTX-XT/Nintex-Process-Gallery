    <Activity Id="65321" UniqueId="64fd4acb-b4f0-455e-9a20-81108d7b4a8e" Number="6.0" Order="22">
      <Text> Create or Update Service Request </Text>
      <Ownerships>
        <Role Id="365" Name="IT Applications Team" IsVirtual="false" />
      </Ownerships>
      <ProcessProcedureTimeframe TimeframeTypeId="-1" TimeframeType="" CycleTimeDisplay="-" LeadTimeDisplay="-" ProportionOfTimeInTotal="0.00000000000" />
      <ChildProcessProcedures>
        <Task Id="65327" UniqueId="69835bb3-bdda-4deb-81ed-b4437da3094b" Order="23" Number="6.0.1">
          <Text>If no service request for issue, create one in the service request system record details of the issue</Text>
        </Task>
        <Task Id="65328" UniqueId="7a340949-6239-44fa-93b9-6926d0657701" Order="24" Number="6.0.2">
          <Text>If existing service request update with actions taken</Text>
        </Task>
        <Task Id="65330" UniqueId="14b4754e-a39f-453d-b5d0-5d33440c3601" Order="25" Number="6.0.3">
          <Text>Prioritise work according to the priority ratings</Text>
        </Task>
        <Note Id="65339" UniqueId="e7d8220a-ca5f-48e2-854a-04ad3ffbb46e" Order="26" Number="">
          <Text>Priority Ratings</Text>
          <Attachment>Detail priority ratings and what constitutes urgent work that takes priority over other work (the show stoppers)</Attachment>
        </Note>
        <Task Id="65342" UniqueId="3c56ed4c-65bc-4364-8bca-af39a580b66c" Order="27" Number="6.0.4">
          <Text>Forward request to appropriate staff person if not yourself</Text>
        </Task>
      </ChildProcessProcedures>
    </Activity>
